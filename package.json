{
  "name": "skeletonspa",
  "version": "1.0.0",
  "description": "Skeleton for a Single Page Application using Gulp and AngularJS",
  "main": "gulpfile.js",
  "keywords": [
    "gulp",
    "angular",
    "single page application",
    "skeleton"
  ],
  "scripts": {
    "lint": "eslint client/ tasks/ ./*.js  --ext .js --cache || true",
    "start": "gulp start",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run",
    "teste2e": "protractor protractor.config.js --baseUrl http://localhost:4000 --specs=./client/**/*spec.e2e.js --stackTrace",
    "webdriver-update": "webdriver-manager update",
    "webdriver": "webdriver-manager start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elgervb/skeletonSPA/"
  },
  "author": {
    "name": "Elger van Boxtel",
    "email": "elgervb@gmail.com",
    "url": "http://elgervanboxtel.nl"
  },
  "bugs": {
    "url": "https://github.com/elgervb/skeletonSPA/issues",
    "email": "elgervb@gmail.com"
  },
  "settings": {
    "src": "./client/",
    "entry": "./client/app/app.js",
    "dist": "./dist",
    "reports": "./reports",
    "serverport": 4000
  },
  "license": "ISC",
  "dependencies": {
    "angular": "^1.5.0",
    "angular-ui-router": "^0.3.1",
    "moment": "^2.13.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.0",
    "babel-core": "^6.7.7",
    "babel-istanbul-loader": "^0.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "browser-sync": "^2.11.1",
    "chai": "^3.4.0",
    "connect-history-api-fallback": "^1.1.0",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "eslint": "^2.8.0",
    "eslint-loader": "^1.3.0",
    "fs-walk": "0.0.1",
    "gulp": "^3.9.0",
    "gulp-consolidate": "^0.2.0",
    "gulp-eslint": "^2.0.0",
    "gulp-iconfont": "^8.0.0",
    "gulp-imagemin": "^3.0.1",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-size": "^2.1.0",
    "gulp-template": "^4.0.0",
    "gulp-todo": "^4.1.0",
    "gulp-util": "^3.0.7",
    "html-webpack-plugin": "^2.21.0",
    "image-webpack-loader": "^1.8.0",
    "jasmine-spec-reporter": "^2.4.0",
    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-opera-launcher": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.4",
    "karma-webpack": "^1.5.1",
    "mocha": "^2.3.0",
    "ng-annotate-loader": "0.1.0",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.5.3",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "run-sequence": "^1.1.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "supports-color": "^3.1.2",
    "vinyl-paths": "^2.1.0",
    "webpack": "^1.9.5",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.6.0",
    "yargs": "^4.7.1"
  }
}
